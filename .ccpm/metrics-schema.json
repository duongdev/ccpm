    "quality": {
      "test_coverage": {
        "description": "Code coverage percentage",
        "unit": "percentage",
        "target": 90,
        "threshold_warning": 80,
        "data": []
      },
      "critical_issues": {
        "description": "Count of critical blocking issues",
        "unit": "count",
        "target_max": 0,
        "data": []
      },
      "regressions": {
        "description": "Count of regression bugs identified",
        "unit": "count",
        "target_max": 0,
        "data": []
      }
